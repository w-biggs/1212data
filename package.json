{
  "name": "1212data",
  "version": "0.1",
  "description": "1212 data & api",
  "main": "index.js",
  "author": "WB",
  "license": "MIT",
  "private": true,
  "scripts": {
    "push": "rsync -rzh --exclude 'node_modules/' --info=progress2 --no-inc-recursive --delete ./ server:/srv/1212data",
    "dumpPush": "rm -r dump/ && mongodump --db 1212 --gzip && rsync -rzh --exclude 'node_modules/' --info=progress2 --no-inc-recursive --delete ./ server:/srv/1212data",
    "dumpGet": "rsync -rzh --info=progress2 --no-inc-recursive --delete server:/srv/1212data/dump ./",
    "dumpRestore": "mongorestore --drop --gzip --db 1212 dump/1212",
    "start": "node server.js",
    "metrics": "node metrics/updateMetrics.js",
    "forfeit": "node tasks/forfeitGame.js"
  },
  "dependencies": {
    "bent": "^7.1.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "mathjs": "^6.6.1",
    "mongoose": "^5.9.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.13",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2"
  }
}
