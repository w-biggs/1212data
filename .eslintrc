{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node": true
  },
  "rules" : {
    "no-console": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id", "_doc"] // Mongoose uses _id and _doc
      }
    ]
  }
}